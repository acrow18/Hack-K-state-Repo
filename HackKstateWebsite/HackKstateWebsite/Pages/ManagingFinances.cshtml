@page
@model ManagingFinancesModel
@{
    ViewData["Title"] = "Managing Finances";
}

<!-- code from https://www.geeksforgeeks.org/search-bar-using-html-css-and-javascript/?ref=rp&tab=article -->
<!DOCTYPE html>
<html>
<head>
    <title>
        Creating Search Bar using HTML
        CSS and Javascript
    </title>

    <!-- linking the stylesheet(CSS) -->
    <link rel="stylesheet" type="text/css" href="/css/browse.css">
</head>
<body>

    <!-- input tag -->
    <input id="searchbar" onkeyup="search_college()" type="text"
           name="search" placeholder="Search Managing Finances...">

    <!-- unordered list -->
    <ul class='collegeList'>
        <!--Student Life-->
        <li class="collegeResources"><h2>Managing Finances</h2><a href="https://www.k-state.edu/careercenter/">Career Center</a></li>
        <li class="collegeResources"><a href="https://www.k-state.edu/nationalscholarships/ ">Office of National Scholarships</a></li>
        <li class="collegeResources"><a href="https://www.k-state.edu/veteran/">Office of Veteran Affairs</a></li>
        <li class="collegeResources"><a href="https://www.k-state.edu/powercatfinancial/">Powercat Financial</a></li>
        <li class="collegeResources"><a href="https://lib.k-state.edu/research-find/textbooks/">Library Course Reserves</a></li>
        <li class="collegeResources"><a href="https://www.k-state.edu/sfa/about/advisor/">Financial Aid advisor</a></li>
        <li class="collegeResources"><a href="https://www.k-state.edu/finsvcs/cashiers/">Cashiers and Student Accounts</a></li>
    </ul>
    <!-- linking javascript -->
    <script src="./js/browse.js"></script>
</body>
</html>